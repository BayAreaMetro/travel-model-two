          ENDIF
          
          ;if skimset 3 doesnt have both local and premium, make it NA
          if(MW[70][J]==0 | (MW[71][J]+MW[72][J]+MW[73][J]+MW[74][J]+MW[75][J])==0)
            MW[61][J] = NA
            MW[62][J] = NA
