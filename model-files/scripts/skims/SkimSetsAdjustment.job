          NA = 0
          
          ;log results if desired
          ;PRINT LIST=I, J, MW[17][J], MW[37][J], MW[57][J], CSV=T, FILE=SkimSetsAdjustmentsLog.csv
          
          ;compare set 1 and 2
          IF (MW[29][J] == MW[59][J])
