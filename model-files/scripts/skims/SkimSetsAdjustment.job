        MW[29] = MW[2] + MW[3] + MW[8] + MW[9] + MW[10] + MW[11] + MW[12] + MW[13] + MW[14] + MW[15]
        MW[59] = MW[32]+ MW[33]+ MW[38]+ MW[39]+ MW[40] + MW[41] + MW[42] + MW[43] + MW[44] + MW[45]
        MW[89] = MW[62]+ MW[63]+ MW[68]+ MW[69]+ MW[70] + MW[71] + MW[72] + MW[73] + MW[74] + MW[75]

        ;output matrices
        MATO[1] = "skims\transit_skims_@TOKEN_PERIOD@_SET1_Adj.TPP",MO=1-16,18-28,
          NAME = COMPCOST,IWAIT,XWAIT,XPEN,BRDPEN,XFERS,FARE,XWTIME,AEWTIME,LB_TIME,EB_TIME,FR_TIME,LR_TIME,HR_TIME,CR_TIME,BEST_MODE,CWDWAITA,CWDWAITP,CWDCOSTP,EXCESSDEMAND,EXCESSPROP,LB_DIST,EB_DIST,FR_DIST,LR_DIST,HR_DIST,CR_DIST
        MATO[2] = "skims\transit_skims_@TOKEN_PERIOD@_SET2_Adj.TPP",MO=31-46,48-58,
          NAME = COMPCOST,IWAIT,XWAIT,XPEN,BRDPEN,XFERS,FARE,XWTIME,AEWTIME,LB_TIME,EB_TIME,FR_TIME,LR_TIME,HR_TIME,CR_TIME,BEST_MODE,CWDWAITA,CWDWAITP,CWDCOSTP,EXCESSDEMAND,EXCESSPROP,LB_DIST,EB_DIST,FR_DIST,LR_DIST,HR_DIST,CR_DIST
        MATO[3] = "skims\transit_skims_@TOKEN_PERIOD@_SET3_Adj.TPP",MO=61-76,78-88,
          NAME = COMPCOST,IWAIT,XWAIT,XPEN,BRDPEN,XFERS,FARE,XWTIME,AEWTIME,LB_TIME,EB_TIME,FR_TIME,LR_TIME,HR_TIME,CR_TIME,BEST_MODE,CWDWAITA,CWDWAITP,CWDCOSTP,EXCESSDEMAND,EXCESSPROP,LB_DIST,EB_DIST,FR_DIST,LR_DIST,HR_DIST,CR_DIST
        
