            MW[88][J] = NA
		  ENDIF

        ENDJLOOP

      ENDRUN
      
      ;rename files
      *del "skims\transit_skims_@TOKEN_PERIOD@_SET1.TPP"
      *rename "skims\transit_skims_@TOKEN_PERIOD@_SET1_Adj.TPP" "skims\transit_skims_@TOKEN_PERIOD@_SET1.TPP"

      *del "skims\transit_skims_@TOKEN_PERIOD@_SET2.TPP"
