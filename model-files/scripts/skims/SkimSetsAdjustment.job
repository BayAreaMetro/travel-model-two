            MW[80][J] = NA
            MW[81][J] = NA
            MW[82][J] = NA
            MW[83][J] = NA
            MW[84][J] = NA
            MW[85][J] = NA
            MW[86][J] = NA
            MW[87][J] = NA
            MW[88][J] = NA
          ENDIF
          
          ;compare set 2 and 3
          IF (MW[59][J] == MW[89][J])
            MW[61][J] = NA
            MW[62][J] = NA
            MW[63][J] = NA
            MW[64][J] = NA
